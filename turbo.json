{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "contracts#build": {
      "outputs": ["types/**"]
    },
    "api#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {},
    "format": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}

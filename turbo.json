{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {},
    "app#build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "api#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {},
    "format": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
